{
    "eslintConfig": {
        "extends": [
            "plugin:react/recommended",
            "airbnb",
            "airbnb/hooks",
            "plugin:prettier/recommended"
          ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "react/jsx-filename-extension": [
            1,
            {
              "extensions": [".js", ".jsx"]
            }
          ]
    }
    }
}